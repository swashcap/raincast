{
  "name": "raincast",
  "version": "1.0.0",
  "description": "Weather and statistics widgets for Portland, Oregon.",
  "main": "dist/main/index.js",
  "scripts": {
    "build": "yarn clean && mkdirp dist && run-p build:*",
    "build:main": "tsc --build tsconfig.main.json",
    "build:render": "NODE_ENV=production webpack",
    "clean": "rimraf dist",
    "format": "prettier --write '*.js' '{src,test,types}/**/*'",
    "start": "run-p watch start:electron",
    "start:electron": "wait-on http://localhost:8080 dist/main/index.js && electron .",
    "storybook": "start-storybook",
    "test": "NODE_ENV=test ava",
    "watch": "run-p watch:*",
    "watch:main": "yarn build:main --watch",
    "watch:render": "webpack-dev-server"
  },
  "keywords": [
    "pdx",
    "portland",
    "weather"
  ],
  "author": "swashcap",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.6.3",
    "@koa/cors": "^3.0.0",
    "connected-react-router": "^6.6.0",
    "debug": "^4.1.1",
    "electron": "^7.0.1",
    "electron-debug": "^3.0.1",
    "electron-is-dev": "^1.1.0",
    "electron-settings": "^3.2.0",
    "electron-window-state": "^5.0.3",
    "get-port": "^5.0.0",
    "grommet": "^2.7.9",
    "grommet-icons": "^4.3.0",
    "history": "^4.10.1",
    "ip": "^1.1.5",
    "koa": "^2.10.0",
    "koa-bodyparser": "^4.2.1",
    "koa-route": "^3.2.0",
    "koa-send": "^5.0.0",
    "moment": "^2.24.0",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "qr-image": "^3.2.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.4.0",
    "xml2js": "^0.4.22"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/plugin-transform-typescript": "^7.7.0",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@babel/register": "^7.6.2",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@storybook/addon-actions": "^5.2.4",
    "@storybook/addon-knobs": "^5.2.4",
    "@storybook/addon-notes": "^5.2.4",
    "@storybook/addons": "^5.2.4",
    "@storybook/react": "^5.2.4",
    "@types/react-redux": "^7.1.5",
    "@types/react-router-dom": "^5.1.2",
    "@types/styled-components": "4.1.8",
    "ava": "^2.4.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.6",
    "css-loader": "^3.2.0",
    "file-loader": "^4.3.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.8",
    "lint-staged": "^9.4.3",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.19.1",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "rimraf": "^3.0.0",
    "style-loader": "^1.0.0",
    "typescript": "^3.7.2",
    "wait-on": "^3.3.0",
    "webpack": "^4.41.1",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2"
  },
  "private": true
}
